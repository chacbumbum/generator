{
    "name": "qsoft/generator",
    "type": "library",
    "description": "Generator angular for Laravel",
    "version": "1.0.0",
    "keywords": [
        "Qsoft",
        "Generator"
    ],
    "homepage": "https://github.com/qsoft/generator",
    "license": "MIT",
    "authors": [{
        "name": "Recycle Bin",
        "email": "thedv91@gmail.com",
        "homepage": "http://chacbumbum.github.io",
        "role": "Developer"
    }],
    "require": {
        "illuminate/support": "~5.2",
        "twig/twig": "^1.24",
        "chumper/zipper": "^0.6.1",
        "php": "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Qsoft\\Generator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qsoft\\Generator\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev"
